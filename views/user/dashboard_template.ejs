<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('../partials/head.ejs') %>
</head>
<body>
    <%- include('../partials/headbar.ejs') %>

    <section id="trackers">
        <div class="inner-wrapper slider-container">
            <div class="card tracker">
                <div class="title">Title Content Goes Here</div>
                <div class="money-sign">$</div>
                <div class="amount">325/<span>$1,256.35</span></div>
                <div class="members">
                    <div class="chip">
                        <img src="/noimg.svg">
                        <span>Member Name</span>
                    </div>
                    <div class="chip">
                        <img src="/noimg.svg">
                        <span>Member Name</span>
                    </div>
                    <span>+ # more</span>
                </div>
            </div>
        </div>
    </section>

    <section id="recents">
        <h3>Recents</h3>
        <div class="inner-wrapper slider-container">
            <div class="card recent">
                <h5>Title</h5>
                <p>Subtitle</p>
                <img src="/noimg.svg">
            </div>
        </div>
    </section>

    <section id="user-summary">
        <div class="card summary">
            <div class="pie-chart">
                <div class="text-content"></div>
                <div class="pie-slices"></div>
            </div>
            <div class="text-content">
                <h4>Title</h4>
                <div class="bullet-li">
                    <div class="pill"></div>
                    <span class="text-content">Some arbitrary text content</span>
                </div>
                <div class="bullet-li">
                    <div class="pill"></div>
                    <span class="text-content">Some arbitrary text content</span>
                </div>
            </div>
        </div>
    </section>

    <%- include('../partials/navbar.ejs') %>
</body>
</html>